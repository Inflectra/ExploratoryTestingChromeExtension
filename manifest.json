{
  "manifest_version": 2,
  "name": "Exploratory Testing Chrome Extension",
  "description": "Exploratory testing session using Chrome",
  "version": "1.0",
  "background": {
    "scripts": [
      "lib/browserInfo.js",
      "lib/date.js",
      "src/Annotation.js",
      "src/Session.js",
      "src/ExportSessionCSV.js",
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
